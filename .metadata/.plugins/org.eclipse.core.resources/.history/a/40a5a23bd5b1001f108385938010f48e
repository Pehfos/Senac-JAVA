package zoologico;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Cachorro cao = new Cachorro();
		Jacare jaca = new Jacare();
		
		
		gato.setCor("Branco");
		gato.setRaca("Vira lata");
		gato.setPeso(-1);
		gato.saltar();
		gato.emitirSom();

	}

}
