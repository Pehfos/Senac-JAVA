package tutorial;

public class Decisao {

	public static void main(String[] args) {
		int idade = 35;
		
		if (idade < 25) {
			System.out.println("Você não pode entrar aqui...");
		}
		
		if (idade < 25) {
			System.out.println("Você não pode entrar aqui...");
		} else {
			System.out.println("Sejam bem vindo!");
		}
	}

}
